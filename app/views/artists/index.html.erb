<h1>All Artists!</h1>
	
<p> <%= link_to 'Add an artist', new_artist_path %> </p>
<% @artists.each do |artist| %>
	
	<div>
		<strong><%= link_to artist.name, artist_path(artist) %></strong>
		<p> <%= artist.bio %></p>
		<p><%= link_to 'Edit', edit_artist_path(artist) %> <%= link_to 'Destroy', artist_path(artist),
			    method: :delete, data:{ confirm: "Seriously?" } %></p>
	</div>

<% end %>


